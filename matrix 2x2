#matrix 2*2
a= [[2,1],[3,4]]
b= [[1,2],[2,3]]

#mulai dari nol supaya memastikan bahwa setiap hasil penjumlahan dimulai dengan benar
hasil_kali = [0,0],[0,0]

#melakukan perkalian matrix
for i in range(len(a)):
    for j in range(len(b[0])):
        for k in range(len(b)):
            hasil_kali[i][j] += a[i][k]*b[k][j]
            
for r in hasil_kali:
    print(r)
